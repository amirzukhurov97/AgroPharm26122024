<br />
<br />
<body class="page-header-fixed sidemenu-closed-hidelogo page-content-white page-md header-white dark-sidebar-color logo-indigo">
    <header>   
		<div class="page-wrapper">
			<!-- start header -->
			<div class="page-header navbar navbar-fixed-top">
				<div class="page-header-inner ">
					<!-- logo start -->
					<div class="page-logo">
                        <a asp-controller="Home" asp-action="Index">
							<span class="logo-default">AgroPharm</span>
						</a>
					</div>
					<!-- logo end -->
					<ul class="nav navbar-nav navbar-left in">
						<li><a href="#" class="menu-toggler sidebar-toggler"><i class="icon-menu"></i></a></li>
					</ul>
					<form class="search-form-opened" action="#" method="GET">
						<div class="input-group">
							<input type="text" class="form-control" placeholder="Search..." name="query">
							<span class="input-group-btn">
								<a href="javascript:;" class="btn submit">
									<i class="icon-magnifier"></i>
								</a>
							</span>
						</div>
					</form>
					<!-- start mobile menu -->
					<a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
						<span></span>
					</a>
					<!-- end mobile menu -->
					<!-- start header menu -->
					<div class="top-menu">
						<ul class="nav navbar-nav pull-right">
							<li><a href="javascript:;" class="fullscreen-btn"><i class="fa fa-arrows-alt"></i></a></li>	
						</ul>
					</div>
				</div>
			</div>
			
			<!-- end header -->			
        <!-- start sidebar menu -->
 			<div class="sidebar-container">
 				<div class="sidemenu-container navbar-collapse collapse fixed-menu">
	                <div id="remove-scroll">
	                    <ul class="sidemenu  page-header-fixed" data-keep-expanded="false" data-auto-scroll="true" data-slide-speed="200" style="padding-top: 20px">
	                        <li class="sidebar-toggler-wrapper hide">
	                            <div class="sidebar-toggler">
	                                <span></span>
	                            </div>
	                        </li>
	                        <li class="nav-item">
	                            <a href="#" class="nav-link nav-toggle">
									<i class="material-icons">dashboard</i>
	                                <span class="title">Dashboard</span>
                                	<span class="selected"></span>
                                	<span class="arrow"></span>
	                            </a>	                            
	                        </li>
							<li class="nav-item">
								<a href="#" class="nav-link nav-toggle">
									<i class="material-icons">add</i>
	                            <span class="title">Добавить</span><span class="arrow"></span></a>
	                            <ul class="sub-menu">
	                                <li class="nav-item">
										<a asp-controller="Product" asp-action="Index" class="nav-link">
											<i class="material-icons">library_add</i>
											<span class="title">Наименование товара</span>
	                                    </a>
	                                </li>	                                
	                                <li class="nav-item">
										<a asp-controller="Organization" asp-action="Index" class="nav-link">
											<i class="material-icons">
												<i class="material-icons">location_city</i>
											</i>
											<span class="title">Наименование организации</span>
	                                    </a>
	                                </li> 
									<li class="nav-item">
										<a asp-controller="Customer" asp-action="Index" class="nav-link">
											<i class="material-icons">person_add</i>
											<span class="title">Наименование покупателя</span>
	                                    </a>
	                                </li> 
	                            </ul>								
	                        </li>
							<li class="nav-item">
								<a class="nav-link nav-toggle" asp-controller="Buy" asp-action="All_BuyProducts">
									<i class="material-icons">add_shopping_cart</i>
	                            <span class="title">Закупка</span><span class="arrow"></span></a>	                       
	                        </li><li class="nav-item">
								<a href="#" class="nav-link nav-toggle">
									<i class="material-icons">shopping_cart</i>
	                            <span class="title">Продажа</span><span class="arrow"></span></a>
	                        </li><li class="nav-item">
								<a href="#" class="nav-link nav-toggle">
									<i class="material-icons">assignment_return</i>
	                            <span class="title">Возврат</span><span class="arrow"></span></a>
	                            <ul class="sub-menu">	                                	                                
	                                <li class="nav-item">
										<a href="add_student_bootstrap.html" class="nav-link ">
											<i class="material-icons">assignment_returned</i>
											<span class="title">От пакупателя</span>
	                                    </a>
	                                </li> 
									<li class="nav-item">
										<a href="add_student_bootstrap.html" class="nav-link ">
											<i class="material-icons">cached</i>
											<span class="title">Организацию</span>
	                                    </a>
	                                </li> 
	                            </ul>
	                        </li>
                            <li class="nav-item">
								<a href="#" class="nav-link nav-toggle">
									<i class="fa fa-dollar"></i>
	                            <span class="title">Курс валют</span><span class="arrow"></span></a>	                            
	                        </li>
                            <li class="nav-item">
								<a href="#" class="nav-link nav-toggle">
                                    <i class="fa fa-exclamation-circle"></i>
	                            <span class="title">О нас</span><span class="arrow"></span></a>	                            
	                        </li>
	                        
	                    </ul>
	                </div>
                </div>
            </div>
		</div>
            <!-- end sidebar menu --> 

    </header>

    <div class="container-fluid" style="margin-top: 20px;">
        <!-- Основной контент будет сдвинут вниз на высоту навбара -->
        <main role="main" class="pb-3">
            @RenderBody()
        </main>

    </div>

    <!-- start footer -->
    <div class="page-footer">
        <div class="page-footer-inner">
            2024 &copy; AgroPharm Theme By
            <a href="mailto:zukhurovamir96@gmail.com" target="_top" class="makerCss"></a>
        </div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>
    <!-- end footer -->


