	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link href="@Url.Content("~/assets/plugins/datatables/plugins/bootstrap/dataTables.bootstrap4.min.css")" rel="stylesheet" type="text/css">

	
